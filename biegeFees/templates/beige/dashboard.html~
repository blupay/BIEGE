 	 	<script type="text/javascript">
var rowNum = 0;
function addRow(frm) {
	rowNum ++;
	var row = '<div class = "form-group" id="rowNum'+rowNum+'"><label for="inputEmail3" class="col-sm-5 control-label">Location </label><div class="col-sm-6"> <input type="text" class = "form-control" name="qty[]" size="4" value="'+frm.add_qty.value+'"></div> <input type="button" class="btn btn-primary" value="Remove" onclick="removeRow('+rowNum+');"></div>';
	jQuery('#itemRows').append(row);
	frm.add_qty.value = '';
	frm.add_name.value = '';
}

function removeRow(rnum) {
	jQuery('#rowNum'+rnum).remove();
}
</script>
    
